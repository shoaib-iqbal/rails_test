<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
       Project form
     </div>
     <div class="panel-body">
      <div class="row">

        <%= form_with(model: project, local: true) do |f| %>
        <% if project.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(project.errors.count, "error") %> prohibited this project from being saved:</h2>

          <ul>
            <% project.errors.full_messages.each do |message| %>
            <li><%= message %></li>
            <% end %>
          </ul>
        </div>
        <% end %>
        <div class="col-md-6">

          <div class="form-group">
            <%=f.label :title%>
            <%=f.text_field :title ,class: 'form-control', placeholder: 'Enter name', required: 'true'%>
          </div>
          <div class="form-group">
          <%=f.label :users%>
            <%= f.select( :user_ids,    options_from_collection_for_select(User.all, :id, :name,f.object.user_ids), {},  {:class => 'select form-control', multiple: true}) %>
          </div>


          <%= f.submit 'Submit Button', class: "btn btn-default" %>
          <button type="reset" class="btn btn-default">Reset Button</button>
          <a  href="/admin/projects" class="btn btn-default">Back</a>
        </div>

        <%end%>
      </div>
    </div>        
  </div>
</div>    
</div>








